<div>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with heading}}
            {{#with main}}
            <h1>{{title}}</h1>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-primary">{{solid}}</button>
            <button class="btn btn-outline-primary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with second}}
            <h2>{{title}}</h2>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-secondary">{{solid}}</button>
            <button class="btn btn-outline-secondary">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with subblock}}
            <h3>{{title}}</h3>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-dark">{{solid}}</button>
            <button class="btn btn-outline-dark">{{outline}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
    {{/with}}
    <br>
    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
            <div class="display-1">{{title}}</div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with children_display}}
                    <div class="display-4">{{title}}</div>
                    <div>{{title}}</div>
                    <br>
                    {{#with button}}
                    <button class="btn btn-outline-warning">{{text}}</button>
                    {{/with}}
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="display-6">{{title}}</div>
            <br>
            {{#with button}}
            <button class="btn btn-info">{{text}}</button>
            {{/with}}
            {{/with}}
        </div>
    </div>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
            <div class="text-uppercase">{{title}}</div>
            <br>
            <div class="text-lowercase">{{text}}</div>
            <br>
            {{#with button}}
            <button class="btn btn-link">{{text}}</button>
            <a>{{url}}</a>
            {{/with}}
            <br>
            <div class="card">
                <div class="card-body">
                    {{#each list_name}}
                    <ul>
                        <li>
                            </a><button class="btn btn-link">{{text}}</button>
                            <a>{{url}}</a>
                        </li>
                    </ul>
                    {{/each}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>
</div>