<div>
    <br>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <div class="text-center">{{description}}</div>
    {{/with}}
    <br>

    {{#with section}}
    {{#with about_us}}
    {{#with title_list}}
    <div class="card">
        <div class="card-body">
            <div class="display-6 text-end">{{main}}</div>
            <h2 class="text-end">{{sub}}</h2>
            {{/with}}
            {{#with button}}
            <button class="btn btn-outline-info">{{text}}</button>
            {{/with}}
        </div>
    </div>
    {{/with}}
    <br>
    <div class="card">
        <div class="card-body">
            {{#with advantage}}
            <div class="display-5">{{title}}</div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with efficiency}}
                    <div class="display-4">{{title}}</div>
                    <br>
                    <h6>{{sub_title}}</h6>
                    <br>
                    <p>{{description}}</p>
                    <br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with innovation}}
                    <div class="text-center display-4">{{title}}</div>
                    <br>
                    <h6 class="text-center">{{sub_title}}</h6>
                    <br>
                    <p class="text-center">{{description}}</p>
                    <br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with collaboration}}
                    <div class="text-end display-4">{{title}}</div>
                    <br>
                    <h6 class="text-end">{{sub_title}}</h6>
                    <br>
                    <p class="text-end">{{description}}</p>
                    <br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            {{/with}}
        </div>
    </div>

    <br>
    <div class="card">
        <div class="card-body">
            {{#with agreement}}
            <div class="display-5">{{title}}</div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with info}}
                    <h5>{{sub_title}}</h5>


                    <br>
                    <div class="card">
                        <div class="card-body">
                            {{#with detail}}
                            <p class="text-end">{{text}}</p>
                            <br>
                            <button class="btn btn-success">{{button}}</button>
                            {{/with}}
                        </div>
                    </div>
                </div>
            </div>
            {{/with}}
            {{/with}}
            {{/with}}
        </div>
    </div>
</div>